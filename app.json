{
  "expo": {
    "name": "Prayse",
    "slug": "prayer-app",
    "scheme": "prayseapp",
    "version": "11.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "owner": "sahag98",
    "icon": "./assets/PRAYSE.png",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/PRAYSE.png",
      "color": "#349eeb"
    },
    "plugins": [
      "@stream-io/video-react-native-sdk",

      ["expo-apple-authentication"],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Prayse access to your photos/videos to allow you to record journals and set profile pictures.",
          "savePhotosPermission": "Allow Prayse to save files to your gallery.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "sahag",
          "project": "react-native",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 24,
            "targetSdkVersion": 35
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you change your profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/noti-image.png",
          "color": "#349eeb"
        }
      ],

      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Allow Prayse to use the microphone to insert prayers.",
          "speechRecognitionPermission": "Allow Prayse to use speech recognition to insert prayers.",
          "androidSpeechServicePackages": [
            "com.google.android.googlequicksearchbox"
          ]
        }
      ],
      [
        "expo-updates",
        {
          "username": "sahag98"
        }
      ],
      "expo-localization",
      "expo-font",
      "expo-secure-store",
      "expo-router",
      "expo-asset",
      "expo-video"
    ],
    "splash": {
      "image": "./assets/prayse-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
